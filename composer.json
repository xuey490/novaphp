    "require": {
        "php": "^8.1",
        "symfony/routing": "^6.0",
        "symfony/http-foundation": "^6.0",
        "symfony/http-kernel": "^7.2",
        "doctrine/annotations": "^2.0",
        "symfony/finder": "^7.3",
        "monolog/monolog": "^3.9",
        "symfony/dependency-injection": "^7.3",
        "symfony/config": "^7.3",
        "topthink/think-orm": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Framework\\": "framework/"
        }
