{
    "name": "your-vendor/your-framework",
    "description": "A lightweight PHP framework with Symfony components",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Your Name",
            "email": "your.email@example.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "doctrine/annotations": "^2.0",
        "monolog/monolog": "3.7.0",
        "nyholm/psr7": "^1.8",
        "psr/http-message": "*",
        "ramsey/uuid": "^4.9",
        "symfony/config": "^7.3",
        "symfony/dependency-injection": "^7.3",
        "symfony/finder": "^7.3",
        "symfony/http-foundation": "^7.3",
        "symfony/http-kernel": "^7.2",
        "symfony/psr-http-message-bridge": "^7.3",
        "symfony/routing": "^6.0",
        "symfony/translation": "^7.3",
        "symfony/yaml": "^7.3",
        "topthink/think-cache": "^3.0",
        "topthink/think-orm": "^3.0"
    },
    "require-dev": {

        "phpunit/phpunit": "*",
        "symfony/var-dumper": "^6.0 || ^7.0"
    },
    "autoload": {
        "psr-4": {
            "Framework\\": "framework/",
            "App\\": "app/"
        },
        "files": [
            "framework/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "platform-check": false,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
