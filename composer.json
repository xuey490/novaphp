{
    "name": "blue2004/novaphp",
    "description": "A lightweight Powerful Fast, Simple, Safty PHP framework with Symfony components",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "blue2004",
            "email": "your.email@example.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "doctrine/annotations": "^2.0",
        "ezyang/htmlpurifier": "^4.18",
        "fig/http-message-util": "^1.1",
        "monolog/monolog": "3.7.0",
        "psr/http-message": "*",
        "psr/http-server-middleware": "^1.0",
        "ramsey/uuid": "^4.9",
        "symfony/cache": "^7.2",
        "symfony/config": "^7.3",
        "symfony/dependency-injection": "^7.3",
        "symfony/dotenv": "^7.2",
        "symfony/finder": "^7.3",
        "symfony/http-foundation": "^7.3",
        "symfony/http-kernel": "^7.2",
        "symfony/mime": "^7.3",

        "symfony/rate-limiter": "^7.2",
        "symfony/routing": "^6.0",
        "symfony/translation": "^7.3",
        "symfony/yaml": "^7.3",
        "topthink/think-cache": "^3.0",
        "topthink/think-orm": "^3.0",
        "twig/twig": "^3.14"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "symfony/var-dumper": "^6.0 || ^7.0"
    },
    "autoload": {
        "psr-4": {
            "Framework\\": "framework/",
            "App\\": "app/"
        },
        "files": [
            "framework/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "platform-check": false,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
